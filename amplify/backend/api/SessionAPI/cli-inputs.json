{
  "version": 1,
  "paths": {
    "/session": {
      "name": "/session",
      "lambdaFunction": "sessionHandler",
      "permissions": {
        "setting": "open"
      }
    },
    "/session/{sessionId}": {
      "name": "/session/{sessionId}",
      "lambdaFunction": "sessionHandler",
      "permissions": {
        "setting": "open"
      }
    }
  }
}